# Note: SAM does not read .env automatically.
# These variables are conveniences for local scripts or manual overrides.

# Deployment target (used by scripts; SAM uses --config-env)
SAM_ENVIRONMENT=development  # development | staging | production

# Parameters that map to template.yml (normally set via --parameter-overrides)
ALLOWED_ORIGINS=https://staging.example.com
FIREBASE_PROJECT_ID=your-staging-project-id
AWS_REGION=ap-northeast-1
